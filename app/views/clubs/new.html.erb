<%= provide(:title, 'Add Club') %>
<h1>Add Club</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@club) do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%= render 'fields', f: f %>

        <%= f.submit "Add My Club", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
