<!-- Probably move the stylesheet to you layout. Also make sure you include the javascript. -->
<%= stylesheet_link_tag "event_calendar" %>
<% provide(:title, "Calendar of Events") %>


  <% if current_user.admin? %>
     <h3><%= current_user.club.name %> Calendar of Events</h3>
      <aside class="span3">
        <%= form_for (@events) do |f| %>
            <%=render 'event_fields',  f: f%>
        <% end %>
      </aside>
    <div class="span4">
    <%= raw(event_calendar) %>
    </div>
<% else %>
      <h2><%= current_user.club.name %> Calendar of Events</h2>
      <div class="span12">
        <%= raw(event_calendar) %>
      </div>
<% end %>
