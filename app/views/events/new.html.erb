<%= provide(:title, 'New Event') %>
<h1>New event</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@event) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'form', f: f %>

    <%= f.submit "Add Event", class: "btn btn-large btn-primary" %>
  <% end %>
  </div>
</div>